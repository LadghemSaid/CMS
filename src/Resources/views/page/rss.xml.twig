{% set base = app_base_url is defined ? app_base_url : (url('piedweb_cms_homepage')|trim('/', 'right')) %}<?xml version="1.0" encoding="UTF-8"?><rss version="2.0">
    <channel>
        <title>{{ logo_alt }}</title>
        <link>{{ base ~ page('') }}</link>

        {% for page in pages %}
            <item>
                <title>
                    {% if page.h1 is defined and page.h1 is not null %}
                        {{ page.h1 }}
                    {% else %}
                        {{ page.title }}
                    {% endif %}
                </title>
                <link>{{ base ~ page(page.getRealSlug) }}</link>
                <pubDate>{{ page.createdAt|date("D, d M Y H:i:s T") }}</pubDate>
            </item>
        {% endfor %}
    </channel>
</rss>
